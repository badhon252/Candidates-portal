{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "next/core-web-vitals",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    // "quotes": ["error", "single"],
    "semi": ["error", "always"],
    // "no-console": "warn",
    "no-unused-vars": "warn",
    "no-var": "error",
    "prefer-const": "error"
  }
}
